{
  "name": "concerts",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "dotenv -- turbo run build",
    "dev": "dotenv -- turbo run dev --parallel",
    "lint": "dotenv -- turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "generate-types": "npx supabase gen types typescript --project-id jdjjodoknqyekyvtepga --schema public > apps/web/types/supabase.ts"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "dotenv-cli": "^7.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
